<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.suneee.oa.dto.OfficeFileDto">    <insert id="add" parameterType="com.suneee.oa.dto.OfficeFileDto">        INSERT INTO sys_office_file        (`id`, `name`, `filename`, `path`, `ext`)        VALUES        (#{id}, #{name}, #{filename}, #{path}, #{ext})    </insert>    <update id="update" parameterType="com.suneee.oa.dto.OfficeFileDto">        UPDATE sys_office_file        SET        `name` = #{name},        `filename` = #{filename},        `path` = #{path},        `ext` = #{ext}        WHERE        `id` = #{id}    </update>    <select id="getAll" resultType="com.suneee.oa.dto.OfficeFileDto">        SELECT `id`, `name`, `filename`, `path`, `ext` FROM sys_office_file    </select>    <select id="getById" resultType="com.suneee.oa.dto.OfficeFileDto">        SELECT `id`, `name`, `filename`, `path`, `ext` FROM sys_office_file        WHERE id = #{id}    </select>    <delete id="delById" parameterType="java.lang.Long">        DELETE FROM sys_office_file WHERE id = #{id}    </delete></mapper>