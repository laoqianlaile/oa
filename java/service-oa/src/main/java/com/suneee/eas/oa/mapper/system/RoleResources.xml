<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.suneee.eas.oa.dao.system.RoleResourcesDao">    <sql id="columns">        roleResId,roleId,resId    </sql>    <select id="findListByRoleId" parameterType="java.lang.Long" resultType="RoleResources">        SELECT rr.*  FROM SYS_ROLE_RES rr        LEFT JOIN sys_res r on r.resid=rr.resid        WHERE rr.roleId=#{roleId}    </select>    <delete id="deleteByRoleId" parameterType="java.lang.Long">        DELETE FROM SYS_ROLE_RES WHERE roleId = #{roleId}    </delete>    <insert id="save" parameterType="RoleResources">        INSERT INTO sys_role_res        (<include refid="columns"/>)        VALUES        (#{roleResId,jdbcType=NUMERIC}, #{roleId,jdbcType=NUMERIC}, #{resId,jdbcType=NUMERIC})    </insert>    <delete id="deleteById" parameterType="java.lang.Long">        DELETE FROM SYS_ROLE_RES        WHERE        roleResId=#{roleResId}    </delete></mapper>