<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.suneee.eas.oa.dao.user.UserRoleDao">    <insert id="save" parameterType="UserRole">        INSERT INTO sys_user_role        (roleId,userId,userRoleId,fullname,account)        VALUES        (#{roleId}, #{userId}, #{userRoleId},        #{fullname}, #{account})    </insert>    <select id="list" resultType="UserRole" parameterType="java.util.Map">        SELECT roleId,userId,userRoleId,fullname,account        FROM SYS_USER_ROLE	urro        WHERE urro.roleId  =#{roleId}        <if test="@Ognl@isNotEmpty(fullname)"> AND fullname=#{fullname} </if>        <if test="@Ognl@isNotEmpty(account)"> AND account=#{account} </if>        <if test="@Ognl@isNotEmpty(searchCondition)">            AND (fullname LIKE #{searchCondition} OR account LIKE CONCAT("%", #{searchCondition}, "%"))        </if>        <if test="@Ognl@isNotEmpty(orderField)">            order by ur.${orderField} ${orderSeq}        </if>        <if test="@Ognl@isEmpty(orderField)">            order by userRoleId  desc        </if>    </select>    <select id="getByUserIdAndRoleId" resultType="UserRole">        SELECT roleId,userId,userRoleId,fullname,account        FROM sys_user_role        WHERE userid = #{userId}        AND roleid = #{roleId}    </select>    <delete id="deleteById" parameterType="java.lang.Long">        DELETE FROM SYS_USER_ROLE        WHERE        userRoleId=#{userRoleId}    </delete></mapper>