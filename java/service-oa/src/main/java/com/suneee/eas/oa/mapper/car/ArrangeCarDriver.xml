<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.suneee.eas.oa.dao.car.ArrangeCarDriverDao">    <insert id="save" parameterType="ArrangeCarDriver">        INSERT INTO oa_arrange_car_driver        (`id`,`arrId`,`carId`,`carName`,`driverId`,`driverName`)        VALUES        (#{id}, #{arrId}, #{carId}, #{carName}, #{driverId}, #{driverName})    </insert>    <delete id="deleteById" parameterType="java.lang.Long">        DELETE FROM oa_arrange_car_driver WHERE id=#{id}    </delete>    <delete id="deleteByArrId" parameterType="java.lang.Long">        DELETE FROM oa_arrange_car_driver WHERE arrId=#{arrId}    </delete>    <select id="findByArrId" parameterType="java.lang.Long">        SELECT `id`,`arrId`,`carId`,`carName`,`driverId`,`driverName`        FROM oa_arrange_car_driver        WHERE arrId=#{arrId}    </select></mapper>