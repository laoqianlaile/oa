<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.suneee.eas.schedule.dao.JobLogDao">    <select id="listAll" resultType="jobLog">        SELECT * FROM sys_job_log        <where>            <if test="@Ognl@isNotEmpty(group)">                AND `group`= #{group}            </if>            <if test="@Ognl@isNotEmpty(jobName)">                AND `jobName`= #{jobName}            </if>        </where>        ORDER BY endTime DESC    </select>    <insert id="save">        INSERT INTO sys_job_log(logId,jobName,triggerName,`group`,startTime,endTime,runtime,content,state) VALUES(#{logId},#{jobName},#{triggerName},#{group},#{startTime},#{endTime},#{runtime},#{content},#{state})    </insert>    <delete id="deleteByIds">        DELETE FROM sys_job_log WHERE logId IN        <foreach collection="ids" index="index" item="item" open="(" separator="," close=")">          #{item}        </foreach>    </delete></mapper>